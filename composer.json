{
    "name": "masterro/laravel-mail-viewer",
    "description": "Easily view in browser outgoing emails.",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Ihoshyn",
            "email": "igoshin18@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "mail",
        "mailable",
        "log"
    ],
    "require": {
        "illuminate/support": "^10.0",
        "illuminate/events": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/view": "^10.0",
        "illuminate/mail": "^10.0",
        "doctrine/dbal": "^3.5"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MasterRO\\MailViewer\\": "src/app",
            "MasterRO\\MailViewer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasterRO\\MailViewer\\Providers\\MailViewerServiceProvider"
            ]
        }
    }
}
